<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- ==================================================================== -->
    <!-- ФАЙЛ: data/budget_role_sequence.xml -->
    <!-- ==================================================================== -->

    <!-- Послідовності для модуля ролей бюджетування -->

    <record id="seq_budget_role_mapping" model="ir.sequence">
        <field name="name">Налаштування ролей бюджетування</field>
        <field name="code">budget.role.mapping</field>
        <field name="prefix">BRM-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_budget_role_template" model="ir.sequence">
        <field name="name">Шаблони ролей бюджетування</field>
        <field name="code">budget.role.template</field>
        <field name="prefix">BRT-</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_budget_role_validation" model="ir.sequence">
        <field name="name">Валідація ролей бюджетування</field>
        <field name="code">budget.role.validation</field>
        <field name="prefix">BRV-</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>